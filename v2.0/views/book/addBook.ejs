<%include ../partials/header%>

<div style="width: 30%; margin: 25px auto">
        <form action="/book" method="POST">
            <div class="form-group">
                <label>Book Name</label>
                <input type="text" class="form-control" name="username" placeholder="Book Name">
            </div>
            <div class="form-group">
                <label>Author</label>
                <input type="text" class="form-control" name="author" placeholder="Author's Name">
            </div>
            <div class="form-group">
                    <label>Edition</label>
                    <input type="text" class="form-control" name="edition" placeholder="Edition">
            </div>
            <div class="form-group">
                    <label>Publisher</label>
                    <input type="text" class="form-control" name="publisher" placeholder="Publisher">
            </div>
            <div class="form-group">
                <label>File input</label>
                <input type="text" class="form-control" name="image" placeholder="Image url">
            </div>
                <button type="submit" class="btn btn-default">Submit</button>
        </form>
    </div>

<%include ../partials/footer%>